        <trans-unit id="tl_module.dlh_googlemap.0">
            <source>Google Map</source>
            <target>Google Map</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap.1">
            <source>Please select a Google Map.</source>
            <target>Veuillez choisir une Google Map.</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_zoom.0">
            <source>Zoom-Factor</source>
            <target>Niveau de zoom.</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_zoom.1">
            <source>Here you can override the zoom factor set in the module for just this implementation.</source>
            <target>Ici vous pouvez redéfinir le niveau de zoom du module qui sera utilisé uniquement par cette implémentation.</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_size.0">
            <source>Dimensions width x height</source>
            <target>Dimensions largeur x hauteur</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_size.1">
