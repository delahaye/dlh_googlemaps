        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_url.0">
            <source>Link address</source>
            <target>Adresse du lien</target>
        </trans-unit>
        <trans-unit id="tl_module.dlh_googlemap_url.1">
            <source>Please enter a web address (http://...) or an insert-tag.</source>
